# Camera Configuration
# Switch between DSLR and MacBook webcam

# DSLR Mode (default - Canon EOS via gphoto2)
# High quality photos, USB connection required
# Leave USE_WEBCAM unset or set to false
USE_WEBCAM=false

# MacBook Webcam Mode
# Built-in FaceTime camera, no external camera needed
# Uncomment to use webcam instead of DSLR
# USE_WEBCAM=true

# Note: Copy this file to .env and set your preferred mode
